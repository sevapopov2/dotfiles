#aliases - quick commands to invoke different options
#vsCode launch alias
alias VSCode="code-insiders"
#git commands aliases
alias gs="git status"
alias ga="git add"
alias gc="git commit"
alias gp="git push"
alias gb="git branch"
alias gco="git checkout"
alias gpo="git push origin"
alias gplo="git pull origin"
#directory history aliases
alias d="dirs -v"
for index ({0..9}) alias "$index"="cd +${index}"; unset index
